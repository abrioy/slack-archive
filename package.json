    "date-fns": "^2.28.0",
    "emoji-datasource-google": "^15.1.2",
    "es-main": "^1.0.2",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.0",
    "lodash-es": "^4.17.21",
    "node-fetch": "^2.6.7",
    "ora": "^6.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.5",
